set title "PlotSortAlgorithmsAfterCyclesWithSortedInput"
set xlabel "n"
set ylabel "cycles"
set autoscale

set terminal png size 1000,800
set output 'SortingAlgorithmCycleSorted.png'

plot    'InsertionSort_Cycle_Plot_Sorted.txt' using 1:2 title 'InsertionSort' with lines ls 1,\
        'QuickSort_Cycle_Plot_Sorted.txt' using 1:2 title 'QuickSort' with lines ls 2,\
        'MergeSort_Cycle_Plot_Sorted.txt' using 1:2 title 'MergeSort' with lines ls 3,\
        'Heapsort_Cycle_Plot_Sorted.txt' using 1:2 title 'Heapsort' with lines ls 4,\
        'Introsort_Cycle_Plot_Sorted.txt' using 1:2 title 'Introsort' with lines ls 5,\
        'sort_Cycle_Plot_Sorted.txt' using 1:2 title 'std::sort' with lines ls 6,\

